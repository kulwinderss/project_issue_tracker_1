<!-- project details on home page -->
<div id="main-page">
    <h1 id="project_h1">Project Details</h1>
    <table cellpadding="0" cellspacing="10" border="0">
        <thead>
            <tr>
                <th>Project Name</th>
                <th>Author Name</th>
                <th>Project Description</th>
                <th></th>
            </tr>
        </thead>
        <% for(i of project11){ %>
            <tr>
                <td><%= i.project_name%></td>
                <td><%= i.project_creator%></td>
                <td><%= i.project_description%></td>
                <td id="showIssue"><a href="/project/issue?projectid=<%=i.id%>"><i class='fas fa-eye' style='font-size:24px'></i> Show Issues</a></td>
            </tr>
        <% } %> 
    </table>
</div>
            